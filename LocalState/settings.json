{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        }
    ],
    "alwaysShowNotificationIcon": true,
    "centerOnLaunch": true,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{1262bae0-fb49-4134-b987-1fd5c5448861}",
    "experimental.rendering.software": true,
    "launchMode": "default",
    "newTabMenu": 
    [
        {
            "type": "remainingProfiles"
        }
    ],
    "profiles": 
    {
        "defaults": 
        {
            "backgroundImage": "%WT_BASE%\\backgrounds\\dst\\windows-terminal-dark.png",
            "backgroundImageAlignment": "bottomRight",
            "backgroundImageOpacity": 0.37,
            "backgroundImageStretchMode": "uniform",
            "colorScheme": "Zenburn",
            "cursorShape": "filledBox",
            "font": 
            {
                "face": "JetBrains Mono",
                "size": 11.0
            },
            "opacity": 93,
            "useAcrylic": false
        },
        "list": 
        [
